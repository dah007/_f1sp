{
    "globalHeaders": {
        "Access-Control-Allow-Origin": "*"
    },
    "mimeTypes": {
        ".geojson": "application/geo+json",
        ".js": "application/javascript",
        ".json": "application/json",
        ".mjs": "application/javascript",
        ".css": "text/css",
        ".html": "text/html"
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/assets*", "/styles*", "/favicon.ico", "/favicon.png", "/manifest.json"]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/data-api/*",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/assets/*",
            "allowedRoles": ["anonymous"]
        }
    ],
    "trailingSlash": "auto"
}
