{
    "mimeTypes": {
        ".js": "application/javascript",
        ".mjs": "application/javascript",
        ".jsx": "application/javascript",
        ".tsx": "application/javascript",
        ".ts": "application/javascript",
        ".css": "text/css"
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/src/assets/*.{png,jpg,gif}",
            "/assets/*.{png,jpg,gif}",
            "/styles/*.{css}"
        ]
    },
    "routes": [
        {
            "route": "/",
            "allowedRoles": [
                "anonymous",
                "authenticated"
            ]
        },
        {
            "route": "/index.html",
            "allowedRoles": [
                "anonymous",
                "authenticated"
            ]
        },
        {
            "route": "/data-api/*",
            "allowedRoles": [
                "anonymous",
                "authenticated"
            ]
        },
        {
            "route": "/graphql",
            "allowedRoles": [
                "anonymous",
                "authenticated"
            ]
        }
    ]
}